{
  "name": "abram",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:update": "jest --updateSnapshot"
